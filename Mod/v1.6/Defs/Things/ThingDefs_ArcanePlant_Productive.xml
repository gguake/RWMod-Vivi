    <costList>
      <VV_SparklingPetal>20</VV_SparklingPetal>
    </costList>

    <comps>
      <li Class="VVRace.CompProperties_RepeatGatherable" />

      <li Class="VVRace.CompProperties_GlowerArcanePlant">
