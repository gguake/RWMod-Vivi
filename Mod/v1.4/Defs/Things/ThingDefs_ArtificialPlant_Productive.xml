  <!-- 번개봉오리 -->
  <ThingDef ParentName="VV_ArtificialPlantProductive">
    <defName>VV_Thunderbud</defName>
    <label>thunderbud</label>
    <description>.</description>
    <descriptionHyperlinks>
      <ThingDef>VV_SparklingPetal</ThingDef>
    </descriptionHyperlinks>
    <tickerType>Normal</tickerType>
    <thingClass>VVRace.BatteryFlower</thingClass>

    <graphicData>
      <texPath>Things/Plants/VV_Thunderbud</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <statBases>
      <MarketValue>450</MarketValue>
      <MaxHitPoints>100</MaxHitPoints>
      <Beauty>10</Beauty>
      <BeautyOutdoors>10</BeautyOutdoors>

      <VV_PlantGatherCooldown>3</VV_PlantGatherCooldown>
      <VV_PlantHoneyGatherYield>1</VV_PlantHoneyGatherYield>
    </statBases>

    <costList>
      <VV_SparklingPetal>30</VV_SparklingPetal>
    </costList>

    <comps>
      <li Class="VVRace.CompProperties_RepeatGatherable" />

      <li Class="VVRace.CompProperties_GlowerFlora">
        <glowRadius>5</glowRadius>
        <glowColor>(213,110,248,0)</glowColor>
      </li>
      <li Class="CompProperties_Battery">
        <storedEnergyMax>400</storedEnergyMax>
        <efficiency>0.5</efficiency>
        <shortCircuitInRain>false</shortCircuitInRain>
        <transmitsPower>true</transmitsPower>
      </li>

      <li Class="VVRace.CompProperties_SensorExplosive">
        <requiredEnergyAmount>125</requiredEnergyAmount>
        <useEnergyAmount>100</useEnergyAmount>
        
        <sensorRadius>6.9</sensorRadius>
        <sensorWorkerClass>VVRace.SensorWorker_Mechanoid</sensorWorkerClass>
        <explosiveRadius>14.9</explosiveRadius>
        <explosiveCooldownTicks>7500~20000</explosiveCooldownTicks>
        <explosiveDamageDef>EMP</explosiveDamageDef>
        <damageAmountBase>70</damageAmountBase>
      </li>
    </comps>

    <building>
      <allowWireConnection>true</allowWireConnection>
    </building>
    
    <modExtensions>
      <li Class="VVRace.ArtificialPlantModExtension">
        <germinateWeight>100</germinateWeight>
        <germinateRare>true</germinateRare>

        <energyCapacity>350</energyCapacity>
        <zeroEnergyDamageByChance>1~3</zeroEnergyDamageByChance>

        <energyConsumeRule Class="VVRace.EnergyRule_Constant">
          <energy>20</energy>
        </energyConsumeRule>
        <energyGenerateRule Class="VVRace.EnergyRule_BatteryStored">
          <energyByBatteryStored>0~40</energyByBatteryStored>
        </energyGenerateRule>
      </li>
    </modExtensions>
  </ThingDef>
  
