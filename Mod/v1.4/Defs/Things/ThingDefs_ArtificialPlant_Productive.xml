  <!-- 물방울꽃 -->
  <ThingDef ParentName="VV_ArtificialPlantProductive">
    <defName>VV_Waterdrops</defName>
    <label>waterdrops</label>
    <description>.</description>
    <descriptionHyperlinks>
      <ThingDef>VV_CoolingPetal</ThingDef>
    </descriptionHyperlinks>

    <graphicData>
      <texPath>Things/Plants/VV_Waterdrops</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <terrainAffordanceNeeded>VV_WaterOrLight</terrainAffordanceNeeded>
    <statBases>
      <MarketValue>450</MarketValue>
      <MaxHitPoints>40</MaxHitPoints>
      <Beauty>10</Beauty>
      <BeautyOutdoors>10</BeautyOutdoors>
      <Flammability>0.1</Flammability>
      
      <VV_PlantGatherCooldown>3</VV_PlantGatherCooldown>
      <VV_PlantHoneyGatherYield>0.5</VV_PlantHoneyGatherYield>
    </statBases>

    <costList>
      <VV_CoolingPetal>30</VV_CoolingPetal>
    </costList>

    <comps>
      <li Class="VVRace.CompProperties_RepeatGatherable" />

      <li Class="VVRace.CompProperties_SensorExplosive">
        <requiredEnergyAmount>50</requiredEnergyAmount>
        <useEnergyAmount>30</useEnergyAmount>
        
        <sensorRadius>6.9</sensorRadius>
        <sensorWorkerClass>VVRace.SensorWorker_Fire</sensorWorkerClass>

        <explosiveRadius>12.9</explosiveRadius>
        <explosiveCooldownTicks>2500~5000</explosiveCooldownTicks>
        <explosiveEffecterDef>ExtinguisherExplosion</explosiveEffecterDef>
        <explosiveDamageDef>Extinguish</explosiveDamageDef>
      </li>
    </comps>
    
    <modExtensions>
      <li Class="VVRace.ArtificialPlantModExtension">
        <germinateWeight>100</germinateWeight>
        <germinateRare>true</germinateRare>

        <energyCapacity>300</energyCapacity>
        <initialEnergyPercent>1.0</initialEnergyPercent>
        <zeroEnergyDamageByChance>1~3</zeroEnergyDamageByChance>

        <energyConsumeRule Class="VVRace.EnergyRule_Constant">
          <energy>10</energy>
        </energyConsumeRule>
        <energyGenerateRule Class="VVRace.EnergyRule_AdjacentWater">
          <energyByOccupiedWaterCell>60</energyByOccupiedWaterCell>
          <energyByAdjacentWaterCell>25</energyByAdjacentWaterCell>
        </energyGenerateRule>
      </li>
    </modExtensions>
  </ThingDef>

  <!-- 빛바라기 -->
  <ThingDef ParentName="VV_ArtificialPlantProductive">
    <defName>VV_Radiantflower</defName>
    <label>radiantflower</label>
    <description>.</description>
    <descriptionHyperlinks>
      <ThingDef>VV_HeatingPetal</ThingDef>
    </descriptionHyperlinks>

    <thingClass>VVRace.SolarFlower</thingClass>
    <tickerType>Rare</tickerType>

    <graphicData>
      <texPath>Things/Plants/VV_Radiantflower</texPath>
      <graphicClass>VVRace.Graphic_Conditional</graphicClass>
    </graphicData>

    <statBases>
      <MarketValue>180</MarketValue>
      <MaxHitPoints>70</MaxHitPoints>
      <Beauty>10</Beauty>
      <BeautyOutdoors>10</BeautyOutdoors>
      
      <VV_PlantGatherCooldown>3</VV_PlantGatherCooldown>
      <VV_PlantHoneyGatherYield>1</VV_PlantHoneyGatherYield>
    </statBases>

    <costList>
      <VV_HeatingPetal>30</VV_HeatingPetal>
    </costList>

    <comps>
      <li Class="VVRace.CompProperties_RepeatGatherable" />

      <li Class="VVRace.CompProperties_GlowerFlora">
        <glowRadius>14</glowRadius>
        <glowColor>(370,370,370,0)</glowColor>
        <overlightRadius>7.0</overlightRadius>
        <glowOnlyRoofed>true</glowOnlyRoofed>
      </li>
      
      <li Class="CompProperties_Schedule">
        <startTime>0.25</startTime>
        <endTime>0.8</endTime>
        <offMessage>Off for plant resting period</offMessage>
      </li>
    </comps>
    <specialDisplayRadius>5.8</specialDisplayRadius>

    <modExtensions>
      <li Class="VVRace.ArtificialPlantModExtension">
        <germinateWeight>100</germinateWeight>
        <germinateRare>true</germinateRare>

        <energyCapacity>400</energyCapacity>
        <zeroEnergyDamageByChance>1~3</zeroEnergyDamageByChance>

        <energyConsumeRule Class="VVRace.EnergyRule_CompositeSum">
          <rules>
            <li Class="VVRace.EnergyRule_GlowerActive">
              <energy>90</energy>
            </li>
            <li Class="VVRace.EnergyRule_Constant">
              <energy>10</energy>
            </li>
          </rules>
        </energyConsumeRule>
        <energyGenerateRule Class="VVRace.EnergyRule_Sunlight">
          <energyFromSunlightRange>0~190</energyFromSunlightRange>
        </energyGenerateRule>
      </li>
    </modExtensions>
  </ThingDef>

  <!-- 풍년화 -->
  <ThingDef ParentName="VV_ArtificialPlantProductive">
