    <label>richflower</label>
    <description>.</description>
    <descriptionHyperlinks>
      <ThingDef>VV_PerfumingPetal</ThingDef>
    </descriptionHyperlinks>
    <tickerType>Rare</tickerType>

    <graphicData>
      <texPath>Things/Plants/VV_Richflower</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <statBases>
      <MarketValue>450</MarketValue>
      <MaxHitPoints>120</MaxHitPoints>
      <Beauty>12</Beauty>
      <BeautyOutdoors>12</BeautyOutdoors>
      
      <VV_PlantGatherCooldown>3</VV_PlantGatherCooldown>
      <VV_PlantHoneyGatherYield>3.2</VV_PlantHoneyGatherYield>
    </statBases>

    <costList>
      <VV_PerfumingPetal>30</VV_PerfumingPetal>
    </costList>

    <comps>
      <li Class="VVRace.CompProperties_RepeatGatherable" />

      <li Class="VVRace.CompProperties_GlowerFlora">
        <glowRadius>3</glowRadius>
        <glowColor>(255,249,155,0)</glowColor>
      </li>
      
      <li Class="VVRace.CompProperties_Terraformer">
        <cooldownTicks>120000~240000</cooldownTicks>
        <radius>2.7</radius>
        <terraforms>
          <li><from>SoftSand</from><to>Sand</to></li>
          <li><from>Sand</from><to>Gravel</to></li>
          <li><from>Gravel</from><to>Soil</to></li>
          <li><from>Soil</from><to>SoilRich</to></li>
          <li><from>MossyTerrain</from><to>SoilRich</to></li>
          <li><from>MarshyTerrain</from><to>SoilRich</to></li>
        </terraforms>
      </li>

    </comps>

    <modExtensions>
      <li Class="VVRace.ArtificialPlantModExtension">
        <germinateWeight>100</germinateWeight>
        <germinateRare>true</germinateRare>

        <energyCapacity>600</energyCapacity>
        <zeroEnergyDamageByChance>1~3</zeroEnergyDamageByChance>

        <energyConsumeRule Class="VVRace.EnergyRule_Constant">
          <energy>50</energy>
        </energyConsumeRule>
        <energyGenerateRule Class="VVRace.EnergyRule_AdjacentPlant">
          <energyByAdjacentPlant>45</energyByAdjacentPlant>
        </energyGenerateRule>
      </li>
    </modExtensions>
  </ThingDef>
  
