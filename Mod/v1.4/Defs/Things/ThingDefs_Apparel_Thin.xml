<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <ThingDef ParentName="ApparelNoQualityBase">
    <defName>VV_BasicWorkerCostume</defName>
    <label>vivi basic worker costume</label>
    <description>.</description>
    <graphicData>
      <texPath>Things/Apparel/BasicWorkerCostume</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <smeltable>false</smeltable>
    <thingCategories>
      <li>ApparelMisc</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>1200</WorkToMake>
      <Mass>0.25</Mass>
      <ArmorRating_Sharp>0.16</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
      <Insulation_Cold>8</Insulation_Cold>
      <Insulation_Heat>8</Insulation_Heat>
      <EquipDelay>1.0</EquipDelay>
    </statBases>
    <apparel Class="VVRace.ApparelPropertiesExt">
      <bodyPartGroups>
        <li>Neck</li>
        <li>Torso</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/BasicWorkerCostume</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>VV_ViviApparel</li>
      </tags>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <developmentalStageFilter>Child, Adult</developmentalStageFilter>
      <bodyTypeWhitelist>
        <li>Child</li>
        <li>Thin</li>
      </bodyTypeWhitelist>
      <fixedBodyType>Thin</fixedBodyType>
    </apparel>
    <colorGenerator Class="ColorGenerator_White" />
  </ThingDef>

