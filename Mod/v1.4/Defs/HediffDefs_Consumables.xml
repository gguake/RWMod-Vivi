<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <HediffDef>
    <defName>VV_RoyalJelly</defName>
    <label>royal jelly effect</label>
    <description>.</description>
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(1,0,0.5)</defaultLabelColor>

    <priceImpact>false</priceImpact>
    <scenarioCanAdd>false</scenarioCanAdd>

    <comps>
      <li Class="HediffCompProperties_Disappears">
        <disappearsAfterTicks>45000~45000</disappearsAfterTicks>
        <showRemainingTime>true</showRemainingTime>
      </li>
      <li Class="HediffCompProperties_ChangeNeed">
        <needDef>VV_RoyalJelly</needDef>
        <percentPerDay>0.015</percentPerDay>
      </li>
    </comps>
  </HediffDef>

  <HediffDef>
    <defName>VV_ViviGrowthBoost</defName>
    <label>growth boost</label>
    <description>.</description>
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(1,0,0.5)</defaultLabelColor>
    
    <priceImpact>false</priceImpact>
    <scenarioCanAdd>false</scenarioCanAdd>

    
    <stages>
      <li>
        <statFactors>
          <GlobalLearningFactor>1.2</GlobalLearningFactor>
        </statFactors>
      </li>
    </stages>

    <comps>
      <li Class="HediffCompProperties_Disappears">
        <disappearsAfterTicks>45000~45000</disappearsAfterTicks>
      </li>
      <li Class="VVRace.HediffCompProperties_ViviGrowthBoost">
        <tickInterval>2000</tickInterval>
        <additionalAgeBoostMultiplier>2.0</additionalAgeBoostMultiplier>
      </li>
    </comps>
  </HediffDef>
</Defs>