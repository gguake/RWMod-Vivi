		<description>Install a fake antennae.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing fake antennae.</jobString>
		<workAmount>2000</workAmount>
    <recipeUsers Inherit="False">
      <li>VV_Vivi</li>
    </recipeUsers>
		<ingredients Inherit="False">
			<li>
				<filter>
					<thingDefs>
						<li>VV_FakeAntennae</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>VV_FakeAntennae</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>VV_Antennae</li>
		</appliedOnFixedBodyParts>
		<addsHediff>VV_FakeAntennae</addsHediff>
	</RecipeDef>

