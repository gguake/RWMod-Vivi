<Defs>
  <!-- 잔불꽃 -->
  <ThingDef ParentName="VV_ArcanePlantProductive">
    <defName>VV_EmberBloom</defName>
    <label>emberbloom</label>
    <description>.</description>
    <descriptionHyperlinks>
      <ThingDef>VV_HeatingPetal</ThingDef>
    </descriptionHyperlinks>

    <graphicData>
      <texPath>Things/Plants/VV_EmberBloom</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <statBases>
      <MarketValue>180</MarketValue>
      <MaxHitPoints>50</MaxHitPoints>
      <Beauty>10</Beauty>
      <BeautyOutdoors>10</BeautyOutdoors>

      <VV_PlantGatherCooldown>3</VV_PlantGatherCooldown>
      <VV_PlantHoneyGatherYield>1</VV_PlantHoneyGatherYield>
    </statBases>

    <costList>
      <VV_HeatingPetal>20</VV_HeatingPetal>
    </costList>

    <comps>
      <li Class="VVRace.CompProperties_RepeatGatherable" />

      <li Class="VVRace.CompProperties_GlowerArcanePlant">
        <glowRadius>10</glowRadius>
        <glowColor>(255,247,138,0)</glowColor>
      </li>

      <li Class="CompProperties_HeatPusher">
        <compClass>VVRace.CompHeatPusherMana</compClass>
        <heatPerSecond>24</heatPerSecond>
        <heatPushMaxTemperature>29</heatPushMaxTemperature>
      </li>
    </comps>
    
    <researchPrerequisites>
      <li>VV_ArcaneBotany</li>
    </researchPrerequisites>

    <modExtensions>
      <li Class="VVRace.ArcanePlantExtension">
        <manaCapacity>150</manaCapacity>
        <zeroManaDamageByChance>1~3</zeroManaDamageByChance>

        <manaConsumeRule Class="VVRace.ManaFluxRule_Constant">
          <mana>10</mana>
        </manaConsumeRule>
        <manaGenerateRule Class="VVRace.ManaFluxRule_Temperature">
          <activeTemperatureRange>32~120</activeTemperatureRange>
          <manaFromTemperatureRange>30~100</manaFromTemperatureRange>
        </manaGenerateRule>

        <passedBulletOverrides>
          <li>
            <thingDef>VV_Bullet_Pea</thingDef>
            <damageDef>Flame</damageDef>
            <amount>3</amount>
            <armorPenetration>0</armorPenetration>
            <chance>0.35</chance>
          </li>
        </passedBulletOverrides>
      </li>

      <li Class="VVRace.GrowArcanePlantData">
        <ingredients>
          <VV_UnknownSeed>1</VV_UnknownSeed>
          <VV_Fertilizer>20</VV_Fertilizer>
          <VV_Pollen>80</VV_Pollen>
          <VV_Vivicream>30</VV_Vivicream>
        </ingredients>

        <totalGrowDays>5</totalGrowDays>
        <maxHealth>150</maxHealth>
        <maxMana>150</maxMana>
        <healthRegenNoDamagedByDays>15</healthRegenNoDamagedByDays>
        <consumedManaByDay>50</consumedManaByDay>

        <successChanceCurve>
          <points>
            <li>(0,0)</li>
            <li>(0.3,0.15)</li>
            <li>(0.5,0.5)</li>
            <li>(0.7,0.8)</li>
            <li>(0.9,1)</li>
          </points>
        </successChanceCurve>
        <healthBonusAmountCurve>
          <points>
            <li>(0,0)</li>
            <li>(0.5,0)</li>
            <li>(0.75,1)</li>
            <li>(1,2)</li>
          </points>
        </healthBonusAmountCurve>
        <cleanlinessBonusAmountCurve>
          <points>
            <li>(-10,0)</li>
            <li>(-0.0001,0)</li>
            <li>(0,1)</li>
            <li>(0.2,2)</li>
            <li>(0.4,3)</li>
            <li>(0.6,5)</li>
          </points>
        </cleanlinessBonusAmountCurve>

        <optimalTemperatureRange>-8~72</optimalTemperatureRange>
        <badTemperatureThresholdDay>0.3</badTemperatureThresholdDay>
        <badTemperatureDamageByDayCurve>
          <points>
            <li>(0,10)</li>
            <li>(0.5,20)</li>
            <li>(1,40)</li>
            <li>(2,80)</li>
            <li>(3,160)</li>
          </points>
        </badTemperatureDamageByDayCurve>

        <manageIntervalTicks>45000</manageIntervalTicks>
        <badManageThresholdDay>1.5</badManageThresholdDay>
        <badManageDamageByDayCurve>
          <points>
            <li>(0,5)</li>
            <li>(1,15)</li>
            <li>(2,25)</li>
          </points>
        </badManageDamageByDayCurve>

        <requiredMinMana>25</requiredMinMana>
        <badManaThresholdDay>0.5</badManaThresholdDay>
        <badManaDamageByDayCurve>
          <points>
            <li>(0,30)</li>
          </points>
        </badManaDamageByDayCurve>
      </li>
    </modExtensions>
  </ThingDef>

